# description: The Twilio phone number to send broadcast SMS from
# format: phone_number
# required: true
TWILIO_PHONE_NUMBER=

# description: Choose a passcode for your app. Users have to use this passcode to send SMS broadcasts
# format: text
# required: true
PASSCODE=

# description: Run application in Test mode. If true, this will not send real SMS to carrier. This will use Magic number '+15005550006' to send
# format: boolean
# required: true
TESTMODE=true

# description: Test Credentials for Twilio account. Found at https://www.twilio.com/console/project/settings
# format: text 
# required: true if TESTMODE=true
TEST_ACCOUNT_SID=ACtttttttttttttttttttttttttttttttt
TEST_AUTH_TOKEN=tttttttttttttttttttttttttttttttt